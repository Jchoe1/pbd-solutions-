public class ALettertoYourself
{
	public static void main (String[] args)
	{
 		System.out.println("-----------------------------------------------");
		System.out.println("|                                         ####|");
		System.out.println("|                                         ####|");
		System.out.println("|                                         ####|");
		System.out.println("|                                             |");
		System.out.println("|                                             |");
		System.out.println("|                                             |");
		System.out.println("|                    Jonathan Smith           |");
		System.out.println("|                    193 Water Way            |");
		System.out.println("|                    Rochman, VI 90123        |");
		System.out.println("|                                             |");
 		System.out.println("-----------------------------------------------");
	}
}

